<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Initialization</servlet-name>
    <servlet-class>Initialization</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Initialization</servlet-name>
    <url-pattern>/Initialization</url-pattern>
  </servlet-mapping>

  <servlet>
  <servlet-name>Editing</servlet-name>
  <servlet-class>Editing</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Editing</servlet-name>
<url-pattern>/edit</url-pattern>
</servlet-mapping>
  <servlet>
    <servlet-name>Deleting</servlet-name>
    <servlet-class>Deleting</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Deleting</servlet-name>
    <url-pattern>/delete</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Likes</servlet-name>
    <servlet-class>Likes</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Likes</servlet-name>
    <url-pattern>/like</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserCheck</servlet-name>
    <servlet-class>UserCheck</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>UserCheck</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PostServlet</servlet-name>
    <servlet-class>PostServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>PostServlet</servlet-name>
    <url-pattern>/take</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Tweet</servlet-name>
    <servlet-class>Tweet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Tweet</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>

</web-app>
